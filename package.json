{
  "name": "ReactNativeStarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator=\"iPhone 8 Plus\"",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "clean": "yarn run clean:build && yarn run clean:watchman && yarn run clean:npm && yarn run clean:packager",
    "clean:build": "rm -rf ./ios/build",
    "clean:npm": "rm -rf ./node_modules && yarn",
    "clean:packager": "react-native start --reset-cache",
    "clean:watchman": "watchman watch-del-all",
    "test": "jest",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "i18next": "^8.4.3",
    "i18next-react-native-language-detector": "^1.0.2",
    "ramda": "^0.24.1",
    "react": "^16.0.0",
    "react-native": "^0.50.1",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-locale-detector": "^1.0.1",
    "react-native-navigation": "^1.1.256",
    "react-native-style-tachyons": "^3.4.2",
    "react-native-vector-icons": "^4.3.0",
    "react-redux": "^5.0.5",
    "recompose": "^0.24.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "2.1.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.51.0",
    "jest": "20.0.4",
    "jest-cli": "^20.0.4",
    "react-test-renderer": "16.0.0-alpha.12",
    "remote-redux-devtools": "^0.5.12"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native)/"
    ]
  }
}
